{
  "openapi": "3.0.4",
  "info": {
    "title": "ViveksHomoeoClinic",
    "version": "1.0"
  },
  "paths": {
    "/Login": {
      "post": {
        "tags": [
          "Login"
        ],
        "parameters": [
          {
            "name": "UserName",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "Password",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponseDTO"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponseDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponseDTO"
                }
              }
            }
          }
        }
      }
    },
    "/api/Patient/GetAll": {
      "get": {
        "tags": [
          "Patient"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PatientDTO"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PatientDTO"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/PatientDTO"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginResponseDTO": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "nullable": true
          },
          "accessToken": {
            "type": "string",
            "nullable": true
          },
          "expiresIn": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PatientDTO": {
        "required": [
          "addressLine1",
          "dob",
          "firstName",
          "genderId",
          "primaryContactNo"
        ],
        "type": "object",
        "properties": {
          "patientId": {
            "type": "integer",
            "format": "int32"
          },
          "firstName": {
            "minLength": 1,
            "type": "string"
          },
          "lastName": {
            "type": "string",
            "nullable": true
          },
          "addressLine1": {
            "minLength": 1,
            "type": "string"
          },
          "addressLine2": {
            "type": "string",
            "nullable": true
          },
          "pinCode": {
            "type": "integer",
            "format": "int32"
          },
          "dob": {
            "type": "string",
            "format": "date"
          },
          "genderId": {
            "type": "integer",
            "format": "int32"
          },
          "genderName": {
            "type": "string",
            "nullable": true
          },
          "primaryContactNo": {
            "minLength": 1,
            "type": "string"
          },
          "secondaryContactNumber": {
            "type": "string",
            "nullable": true
          },
          "maritalStatusId": {
            "type": "integer",
            "format": "int32"
          },
          "maritalStatus": {
            "type": "string",
            "nullable": true
          },
          "attenderName": {
            "type": "string",
            "nullable": true
          },
          "attenderRelationship": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Enter your JWT Access Token",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}